msgstr "  reinstall <имя>             Переустановить указанный пакет\n"

#: alteratorctlpackagesmodule.c:3260
msgid "  remove <name>               Remove specified package\n"
msgstr ""
"  remove <имя>                Удалить указанный пакет\n"
"\n"

#: alteratorctlpackagesmodule.c:3261
