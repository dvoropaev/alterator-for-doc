#: alteratorctlservicesmodule.c:7388
msgid ""
"  alteratorctl services [OPTIONS]\n"
"                              List services\n"
msgstr ""
"  alteratorctl services [ОПЦИИ]\n"
"                              Вывод списка сервисов\n"
"\n"

#: alteratorctlservicesmodule.c:7390
msgid ""
"  alteratorctl services [COMMAND] [OPTIONS] [PARAMETERS <argument>] "
"[service]\n"
"\n"
msgstr ""
"  alteratorctl services [КОМАНДА] [ОПЦИИ] [ПАРАМЕТРЫ <аргументы>] [сервис]\n"
"\n"

#: alteratorctlservicesmodule.c:7392
msgid "  list                        List services (default command)\n"
msgstr ""
"  list                        Вывести список сервисов (команда по "
"умолчанию)\n"

#: alteratorctlservicesmodule.c:7393
msgid ""
"  status <service>            Get current state and parameters of specified\n"
"                              service\n"
msgstr ""
"  status <сервис>             Получить текущее состояние и параметры "
"указанного\n"
"                              сервиса\n"

#: alteratorctlservicesmodule.c:7395
msgid "  info <service>              Get info about specified service\n"
msgstr "  info <сервис>               Вывести информацию о выбранном сервисе\n"

#: alteratorctlservicesmodule.c:7396
msgid "  resources <service>         Show resources of specified service\n"
msgstr "  resources <сервис>          Вывести ресурсы указанного сервиса\n"

#: alteratorctlservicesmodule.c:7397
msgid ""
"  play <file>                 Deploy service using parameters from JSON "
"file\n"
"                              exported from Alt-Services\n"
msgstr ""
"  play <файл>                 Развернуть сервис, используя параметры из\n"
"                              JSON-файла, экспортированного из Alt-Services\n"

#: alteratorctlservicesmodule.c:7399
msgid ""
"  diagnose <service> (<diagnostic tool> [test...])... [PARAMETERS]\n"
"                              Run diagnostics tests for specified service\n"
msgstr ""
"  diagnose <сервис> (<диагностический инструмент> [тест...])... [ПАРАМЕТРЫ]\n"
"                              Запустить диагностические тесты указанного "
"сервиса\n"

#: alteratorctlservicesmodule.c:7401
msgid ""
"  deploy <service> [PARAMETERS]\n"
"                              Deploy specified service\n"
msgstr ""
"  deploy <сервис> [ПАРАМЕТРЫ]\n"
"                              Развернуть указанный сервис\n"

#: alteratorctlservicesmodule.c:7403
msgid ""
"  configure <service> <PARAMETERS>\n"
"                              Configure specified service\n"
msgstr ""
"  configure <сервис> <ПАРАМЕТРЫ>\n"
"                              Настроить указанный сервис\n"

#: alteratorctlservicesmodule.c:7405
msgid "  start <service>             Start service\n"
msgstr "  start <сервис>              Запустить указанный сервис\n"

#: alteratorctlservicesmodule.c:7406
msgid "  stop <service>              Stop service\n"
msgstr "  stop <сервис>               Остановить указанный сервис\n"

#: alteratorctlservicesmodule.c:7407
msgid ""
"  backup <service> [PARAMETERS]\n"
"                              Backup specified service\n"
msgstr ""
"  backup <сервис> [ПАРАМЕТРЫ]\n"
"                              Создать резервную копию указанного сервиса\n"

#: alteratorctlservicesmodule.c:7409
msgid ""
"  restore <service> [PARAMETERS]\n"
"                              Restore specified service\n"
msgstr ""
"  restore <сервис> [ПАРАМЕТРЫ]\n"
"                              Восстановить указанный сервис\n"

#: alteratorctlservicesmodule.c:7411
msgid ""
"  undeploy <service> [PARAMETERS]\n"
"                              Undeploy specified service\n"
"\n"
msgstr ""
"  undeploy <сервис> [ПАРАМЕТРЫ]\n"
"                              Деактивировать указанный сервис\n"
"\n"

#: alteratorctlservicesmodule.c:7414
msgid ""
"  -v, --verbose               Add services paths to services list output\n"
msgstr ""
"  -v, --verbose               Добавить к списку сервисов пути к их объектам "
"на D-Bus\n"

#: alteratorctlservicesmodule.c:7415
msgid "  -d, --display-name-only     Show only services display names\n"
msgstr ""
"  -d, --display-name-only     Показать только отображаемые имена сервисов\n"

#: alteratorctlservicesmodule.c:7416
msgid "  -D, --no-display-name       Show services without display names\n"
msgstr "  -D, --no-display-name       Скрыть отображаемые имена сервисов\n"

