#: alteratorctldiagmodule.c:672 alteratorctlservicesmodule.c:3759
#: alteratorctlservicesmodule.c:7387
msgid "Usage:\n"
msgstr "Использование:\n"

#: alteratorctlcomponentsmodule.c:1929
msgid ""
"  alteratorctl components [COMMAND [arguments]] [OPTIONS] [PARAMETERS "
"<args...>]\n"
msgstr ""
"  alteratorctl components [КОМАНДА [аргументы]] [ОПЦИИ] [ПАРАМЕТРЫ\n"
"                              <аргументы...>]\n"

#: alteratorctlcomponentsmodule.c:1930
msgid ""
"  alteratorctl components     List all components in a preudo-graphic tree "
"view\n"
"\n"
msgstr ""
"  alteratorctl components     Вывод всех компонентов в виде дерева с\n"
"                              псевдографикой\n"
"\n"

#: alteratorctlcomponentsmodule.c:1931 alteratorctleditionsmodule.c:1550
#: alteratorctlmanagermodule.c:853 alteratorctlremotemodule.c:751
#: alteratorctlsysteminfomodule.c:3191 alteratorctlpackagesmodule.c:3219
#: alteratorctlpackagesmodule.c:3234 alteratorctlpackagesmodule.c:3259
#: alteratorctldiagmodule.c:677 alteratorctlservicesmodule.c:7391
msgid "Commands:\n"
msgstr "Команды:\n"

#: alteratorctlcomponentsmodule.c:1932
msgid "  description <component>     Prints component description\n"
msgstr "  description <компонент>     Вывести описание компонента\n"

#: alteratorctlcomponentsmodule.c:1933
msgid ""
"  list [OPTIONS] [PARAMETERS <args...>]\n"
"                              List all components (default command)\n"
msgstr ""
"  list [ОПЦИИ] [ПАРАМЕТРЫ <аргументы..>]\n"
"                              Вывод всех компонентов с категориями (команда "
"по\n"
"                              умолчанию)\n"

#: alteratorctlcomponentsmodule.c:1935
msgid "  info <component>            Print component info\n"
msgstr "  info <компонент>            Вывести информацию о компоненте\n"

#: alteratorctlcomponentsmodule.c:1936
msgid "  install <component>         Install component\n"
msgstr "  install <компонент>         Установить компонент\n"

#: alteratorctlcomponentsmodule.c:1937
msgid "  remove <component>          Remove component\n"
msgstr "  remove <компонент>          Удалить компонент\n"

#: alteratorctlcomponentsmodule.c:1938
msgid ""
"  status <component>          View the installation status of component\n"
msgstr "  status <компонент>          Посмотреть статус установки компонента\n"

#: alteratorctlcomponentsmodule.c:1939
msgid ""
"  search <component>          Search component by name or path\n"
"\n"
msgstr ""
"  search <компонент>          Найти компонент по имени или пути на D-Bus\n"
"\n"

#: alteratorctlcomponentsmodule.c:1940
msgid "Parameters:\n"
msgstr "Параметры:\n"

#: alteratorctlcomponentsmodule.c:1941
msgid ""
"  -c, --category <category>   Display the list of components of the "
"selected\n"
"                              category\n"
msgstr ""
"  -c, --category <категория>  Вывести список компонентов выбранной "
"категории\n"

#: alteratorctlcomponentsmodule.c:1943
msgid ""
"  -s, --sections <section1> [section2 ...]\n"
"                              Display components and categories list from "
"chosen\n"
"                              sections\n"
"\n"
msgstr ""
"  -s, --sections <section1> [section2 ...]\n"
"                              Отобразить только выбранные разделы\n"
"\n"

#: alteratorctlcomponentsmodule.c:1946
msgid "Options for formatting output:\n"
msgstr "Опции для форматирования вывода:\n"

#: alteratorctlcomponentsmodule.c:1947
msgid ""
"  -v, --verbose               Add components paths to components list "
"output\n"
msgstr ""
"  -v, --verbose               Добавить вывод путей к компонентам на DBus\n"

#: alteratorctlcomponentsmodule.c:1948
msgid ""
"  -d, --display-name-only     Show only components and categories display "
"names\n"
msgstr ""
"  -d, --display-name-only     Показать только отображаемые имена компонентов "
"и\n"
"                              категорий\n"

#: alteratorctlcomponentsmodule.c:1949
